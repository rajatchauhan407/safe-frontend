{
  "expo": {
    "name": "safe-frontend",
    "slug": "safe-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.safesitealert.safefrontend"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.safesitealert.safefrontend",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0db07e18-056c-4902-ba4f-608713c6c8ef"
      },
      "apiUrl": "https://safesitealert.com/api/v1/"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(SAFE APP) to access your camera",
          "microphonePermission": "Allow $(SAFE APP) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ]
  }
}
